<?xml version="1.0" encoding="utf-8"?>
<com.prebunking.game.presentation.view.ZigZagBadgesList xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/zigzag_vertical_list_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/zigzag_vertical_list_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/zigzag_vertical_list_1"
        app:layout_constraintTop_toTopOf="@id/zigzag_vertical_list_1" />

</com.prebunking.game.presentation.view.ZigZagBadgesList>